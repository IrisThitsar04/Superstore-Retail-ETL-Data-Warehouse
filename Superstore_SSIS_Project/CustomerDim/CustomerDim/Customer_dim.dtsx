                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT cp.CustomerID,cp.CustomerName, cp.Segment,ca.Region, ca.Country,ca.City, ca.State, ca.PostalCode, MIN(o.OrderDate) AS FirstOrderDate, MAX(o.OrderDate) AS LatestOrderDate 
FROM CustomerProfile cp
INNER JOIN CustomerAddress ca 
ON cp.ProfileID=ca.ProfileID
INNER JOIN Orders o 
