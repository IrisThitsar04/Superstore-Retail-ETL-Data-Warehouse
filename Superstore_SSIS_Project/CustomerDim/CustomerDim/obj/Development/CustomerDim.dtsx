              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH RankedProfiles AS (
    SELECT *,
        ROW_NUMBER() OVER (PARTITION BY CustomerID ORDER BY CustomerName) AS rn
    FROM CustomerProfile
),
RankedAddresses AS (
    SELECT *,
        ROW_NUMBER() OVER (PARTITION BY CustomerID ORDER BY City) AS rn
    FROM CustomerAddress
)
SELECT
    rp.CustomerID,
    rp.CustomerName,
    rp.Segment,
    ra.City,
    ra.State,
    ra.Country,
    ra.Region,
    ra.PostalCode
FROM RankedProfiles rp
JOIN RankedAddresses ra ON rp.CustomerID = ra.CustomerID
WHERE rp.rn = 1 AND ra.rn = 1;</property>
